[package]
name = "redox-scheme"
authors = ["4lDO2 <4lDO2@protonmail.com>"]
version = "0.8.3"
edition = "2021"
license = "MIT"
exclude = ["target"]
repository = "https://gitlab.redox-os.org/redox-os/redox-scheme"
description = "Library for writing Redox scheme daemons"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[features]
# TODO: hashbrown separately?
default = ["std"]
std = []

[dependencies]
redox_syscall = "0.6.0"
libredox = { version = "0.1.11", default-features = false, features = ["redox_syscall", "call"] }
